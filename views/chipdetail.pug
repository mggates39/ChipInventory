extends layout

block content
  h1= chip.chip_number
  p #[strong Description] #{chip.description}
  p #[strong Package] #{chip.package}
  p #[strong Pin Count] #{chip.pin_count}
  p
  table.table.table-striped
    tr
      th Pin
      th Symbol
      th Description

    each n in pins
      tr
        td= n.pin_number
        td= n.pin_symbol
        td= n.pin_description
